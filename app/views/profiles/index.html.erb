<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Building</th>
      <th>Street</th>
      <th>City</th>
      <th>Pincode</th>
      <th>Mobile</th>
      <th>Dateofbirth</th>
      <th>Email</th>
      <th>Idproof</th>
      <th>Member</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.firstName %></td>
        <td><%= profile.lastName %></td>
        <td><%= profile.building %></td>
        <td><%= profile.street %></td>
        <td><%= profile.city %></td>
        <td><%= profile.pincode %></td>
        <td><%= profile.mobile %></td>
        <td><%= profile.dateOfBirth %></td>
        <td><%= profile.email %></td>
        <td><%= profile.idProof %></td>
        <td><%= profile.member %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
