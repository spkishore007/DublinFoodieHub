<li> <h1> <%= link_to "Go To Admin Dashboard >>>>",rails_admin_path, class:"btn btn-primary btn-lg" %></h1></li> 
<hr>
<%= form_tag do %>
  
  <div class="field">
    Choose your table
    <%= select_tag(:table, options_for_select([['Month Sales','msales'], ['Month Sales With Offers','msalesoffer'], ['Month Sales With Product Type','msalestype'], ['Pizza Pricing','pizza'], ['Pizza Ranking and Counts','hit'], ['Day Sales','dsales'], ['Day Sales With Offers','dsalesoffer'], ['Day Sales With Product Type','dsalestype'], ['Week Sales','wsales'], ['Week Sales With Offers','wsalesoffer'], ['Week Sales With Product Type','wsalestype'], ['Returning Customers','customer']])) %>
  </div>

  <div class="field">
    Choose the type of chart
    <%= select_tag(:chart, options_for_select([['Pie',"pie"], ['Spline',"spline"], ['Bar',"bar"], ['line',"line"]])) %>
  </div>

  <div class="field">
    Single Chart:
    <%= check_box_tag(:singleChart, "single chart") %>
  </div>
  <div class="field">
    Double Chart:
    <%= check_box_tag(:doubleChart, "double chart") %>
  </div>
  <div class="field">
    Triple chart:
    <%= check_box_tag(:tripleChart, "triple chart") %>
  </div>
  <div class="actions">
    <%= submit_tag 'view' %>
  </div>
<% end %>
<hr>
<h1>Hi der! Check out the Visualization</h1>
<% if @chart1 %>
<%= high_chart("my_combine_div1", @chart1) %>
<% end %>
<% if @chart2 %>
<%= high_chart("my_combine_div2", @chart2) %>
<% end %>
<% if @chart3 %>
<%= high_chart("my_combine_div3", @chart3) %>
<% end %>